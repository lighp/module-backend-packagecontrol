[
	{
		"url":"/packagecontrol/about/?",
		"action":"about"
	},
	{
		"url":"/packagecontrol/show/([a-zA-Z0-9-_.]+(/[a-zA-Z0-9-_.]+)?)/?",
		"action":"showPackage",
		"vars": ["name"]
	},
	{
		"url":"/packagecontrol/search/?",
		"action":"searchPackage"
	},
	{
		"url":"/packagecontrol/install/([a-zA-Z0-9-_.]+(/[a-zA-Z0-9-_.]+)?)/?",
		"action":"installPackage",
		"vars": ["name"]
	},
	{
		"url":"/packagecontrol/remove/([a-zA-Z0-9-_.]+(/[a-zA-Z0-9-_.]+)?)/?",
		"action":"removePackage",
		"vars": ["name"]
	},
	{
		"url":"/packagecontrol/upgrade/?",
		"action":"upgradePackages"
	},
	{
		"url":"/packagecontrol/repos/?",
		"action":"listRepositories"
	},
	{
		"url":"/packagecontrol/repos/add/?",
		"action":"addRepository"
	},
	{
		"url":"/packagecontrol/repos/remove/([a-zA-Z0-9-_.]+)/?",
		"action":"removeRepository",
		"vars": ["name"]
	}
]